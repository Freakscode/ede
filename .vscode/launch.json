    "version": "0.2.0",
    "configurations": [
        {
            "name": "Development",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "--dart-define-from-file",
                ".env/development.env"
            ]
        },
        {
            "name": "Production",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart",
            "args": [
                "--dart-define-from-file",
                ".env/production.env"
            ]
        },    
        {
            "name": "ede_final_app",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "ede_final_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "ede_final_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}